{
  "name": "caltrain-api",
  "version": "0.0.1",
  "description": "A simple API for Caltrain",
  "main": "server/index.js",
  "scripts": {
    "babel-node": "babel-node --experimental",
    "test": "export NODE_ENV='test'; ./node_modules/mocha/bin/mocha --compilers js:babel/register server/test/index.js",
    "dev": "./node_modules/nodemon/bin/nodemon.js --exec npm run babel-node -- server/index.js",
    "activate": "source venv/bin/activate",
    "import": "python scraper -g -i -db caltrain_test && python ./scraper -g -i",
    "start": "node ./server",
    "docs": "cp README.md docs/index.md && ./node_modules/docco/bin/docco --layout=linear ./docs/index.md"
  },
  "keywords": [
    "caltrain",
    "api",
    "node",
    "rethinkdb"
  ],
  "author": "thejsj",
  "license": "ISC",
  "devDependencies": {
    "mocha": "^2.1.0",
    "nodemon": "^1.3.7",
    "should": "^5.0.1",
    "supertest": "^0.15.0",
    "supertest-as-promised": "^1.0.0"
  },
  "dependencies": {
    "MD5": "^1.2.1",
    "babel": "^4.7.12",
    "body-parser": "^1.12.0",
    "compression": "^1.4.2",
    "config": "^1.12.0",
    "crypto": "0.0.3",
    "docco": "^0.7.0",
    "express": "^4.12.0",
    "lodash": "^3.3.1",
    "moment": "^2.9.0",
    "q": "^1.2.0",
    "rethinkdb": "^1.16.2",
    "twit": "^1.1.20",
    "universal-analytics": "^0.3.6"
  }
}
